Time.parse_ms(ms: U64): Time
  Time.new(
    U64.div(ms, 86400000l),               // days
    U64.mod(U64.div(ms, 3600000l), 24l),  // hours
    U64.mod(U64.div(ms, 60000l), 60l),    // minutes
    U64.mod(U64.div(ms, 1000l), 60l),     // seconds
    U64.mod(ms, 1000l),                   // milliseconds
    U64.mod(U64.mul(ms, 1000l), 1000l),   // microseconds
    U64.mod(U64.mul(ms, 1000000l), 1000l) // nanoseconds
  )