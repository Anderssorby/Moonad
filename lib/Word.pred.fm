Word.pred<size: Nat>(word: Word(Nat.succ(size))): Word(size)
  word<(size) () Word(Nat.pred(size))>
  | Word.nil;
  | <word.size> (word.pred) word.pred;
  | <word.size> (word.pred) word.pred;

